<%= simple_form_for(@imovel) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tipo %>
    <%= f.input :logradouro %>
    <%= f.input :numero %>
    <%= f.input :bairro %>
    <%= f.input :cidade %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
