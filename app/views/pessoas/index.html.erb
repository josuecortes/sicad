<p id="notice"><%= notice %></p>

<h1>Listing Pessoas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Nascimento</th>
      <th>Sexo</th>
      <th>Rg</th>
      <th>Cpf</th>
      <th>Titulo</th>
      <th>Zona</th>
      <th>Secao</th>
      <th>Logradouro</th>
      <th>Numero</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Contato</th>
      <th>Email</th>
      <th>Entidade</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pessoas.each do |pessoa| %>
      <tr>
        <td><%= pessoa.nome %></td>
        <td><%= pessoa.nascimento %></td>
        <td><%= pessoa.sexo %></td>
        <td><%= pessoa.rg %></td>
        <td><%= pessoa.cpf %></td>
        <td><%= pessoa.titulo %></td>
        <td><%= pessoa.zona %></td>
        <td><%= pessoa.secao %></td>
        <td><%= pessoa.logradouro %></td>
        <td><%= pessoa.numero %></td>
        <td><%= pessoa.bairro %></td>
        <td><%= pessoa.cidade %></td>
        <td><%= pessoa.contato %></td>
        <td><%= pessoa.email %></td>
        <td><%= pessoa.entidade_id %></td>
        <td><%= pessoa.usuario_id %></td>
        <td><%= link_to 'Show', pessoa %></td>
        <td><%= link_to 'Edit', edit_pessoa_path(pessoa) %></td>
        <td><%= link_to 'Destroy', pessoa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pessoa', new_pessoa_path %>
