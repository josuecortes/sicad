<p id="notice"><%= notice %></p>

<h1>Listing Entidades</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Data criacao</th>
      <th>Codigo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entidades.each do |entidade| %>
      <tr>
        <td><%= entidade.nome %></td>
        <td><%= entidade.data_criacao %></td>
        <td><%= entidade.codigo %></td>
        <td><%= link_to 'Show', entidade %></td>
        <td><%= link_to 'Edit', edit_entidade_path(entidade) %></td>
        <td><%= link_to 'Destroy', entidade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entidade', new_entidade_path %>
